{
    "name": "homework",
    "version": "1.0.0",
    "description": "Advanced React",
    "main": "lib/server.js",
    "author": "Sam Niyazi",
    "license": "MIT",
    "private": true,
    "scripts": {
        "dev-hold": "nodemon lib/server.js  --watch lib  --watch views -e ejs,js",
        "dev_hold2": "nodemon lib/server.js  ",
        "dev": "set NODE_PATH=./lib && nodemon  --exec babel-node  lib/server.js  ",
        "webpack": "webpack -wd",
        "test": "jest --watch",
        "verify-test": "jest --coverage",
        "build-webpack": "webpack -p",
        "build-node": "babel lib -d build --copy-files"
    },
    "babel_v01": {
        "presets": [
            "react",
            "env",
            "stage-2"
        ]
    },
    "babel": {
        "presets": [
            "react",
            [
                "env",
                {
                    "targets": {
                        "node": "current"
                    }
                }
            ]
        ],
        "plugins": [
            "transform-class-properties",
            "transform-object-rest-spread"
        ]
    },
    "devDependencies": {
        "@testing-library/react": "^10.4.7",
        "@typescript-eslint/eslint-plugin": "^3.7.0",
        "@typescript-eslint/parser": "^3.7.0",
        "babel-eslint": "^10.1.0",
        "enzyme": "^3.11.0",
        "enzyme-adapter-react-16.3": "^1.7.2",
        "eslint": "^7.5.0",
        "eslint-config-standard": "^14.1.1",
        "eslint-plugin-import": "^2.22.0",
        "eslint-plugin-node": "^11.1.0",
        "eslint-plugin-promise": "^4.2.1",
        "eslint-plugin-react": "^7.20.3",
        "eslint-plugin-standard": "^4.0.1",
        "import": "^0.0.6",
        "react-addons-test-utils": "^15.6.2",
        "webpack-cli": "^3.3.12"
    },
    "dependencies": {
        "axios": "^0.19.2",
        "babel-cli": "^6.26.0",
        "babel-loader": "7",
        "babel-polyfill": "^6.26.0",
        "babel-preset-env": "^1.7.0",
        "babel-preset-react": "^6.24.1",
        "babel-preset-stage-2": "^6.24.1",
        "ejs": "^3.1.3",
        "express": "^4.17.1",
        "jest": "^20.0.4",
        "lodash.debounce": "^4.0.8",
        "lodash.pickby": "^4.6.0",
        "node-fetch": "^2.6.0",
        "react": "^16.13.1",
        "react-dom": "^16.13.1",
        "react-test-renderer": "^16.13.1",
        "webpack": "^4.43.0"
    }
}
